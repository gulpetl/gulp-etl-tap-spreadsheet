{
    "name": "gulp-etl-tap-spreadsheet",
    "version": "1.0.3",
    "repository": "https://github.com/gulpetl/gulp-etl-tap-spreadsheet.git",
    "description": "Parse spreadsheets (XLS, XLSX, ODS, DBF, CSV, etc) into gulp-etl's Message Stream JSON format",
    "main": "dist/js/plugin.js",
    "typings": "dist/types/plugin.d.ts",
    "files": [
        "dist"
    ],
    "scripts": {
        "build": "tsc",
        "prebuild": "npm run clean",
        "clean": "rimraf dist/*",
        "test": "jest",
        "defaultTask": "gulp --gulpfile ./debug/gulpfile.ts default"
    },
    "author": "peterl@sbcotton.com",
    "contributors": [
        "chloe.osgood@ttu.edu"
    ],
    "license": "MIT",
    "keywords": [
        "gulpplugin",
        "csv",
        "spreadsheet",
        "excel",
        "office",
        "xls",
        "xlsx",
        "ods",
        "dbf",
        "gulp-etl"
    ],
    "homepage": "https://gulpetl.com",
    "dependencies": {
        "@types/through2": "^2.0.34",
        "csv-parse": "^4.4.1",
        "embedme": "^1.15.0",
        "from2": "^2.3.0",
        "gulp-error-handle": "^1.0.1",
        "loglevel": "^1.6.1",
        "pkginfo": "^0.4.1",
        "plugin-error": "^1.0.1",
        "through2": "^3.0.1",
        "vinyl": "^2.2.0",
        "xlsx": "^0.14.3"
    },
    "devDependencies": {
        "@types/del": "^4.0.0",
        "@types/gulp-rename": "0.0.33",
        "@types/jest": "^23.3.14",
        "@types/loglevel": "^1.6.3",
        "@types/node": "^10.14.12",
        "@types/vinyl": "^2.0.3",
        "@types/vinyl-paths": "0.0.31",
        "gulp": "^4.0.2",
        "gulp-rename": "^1.4.0",
        "jest": "^24.8.0",
        "jest-spec-reporter": "^1.0.6",
        "rimraf": "^2.6.3",
        "ts-jest": "^24.0.2",
        "ts-node": "^7.0.1",
        "typescript": "^3.5.3"
    }
}
